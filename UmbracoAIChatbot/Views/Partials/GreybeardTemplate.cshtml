@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage@{    Layout = null;}<!DOCTYPE html><html lang="en"><head>    <meta charset="UTF-8">    <meta name="viewport" content="width=device-width, initial-scale=1.0">    <title>Greybeard - Creative Excellence</title>    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css">    <link rel="preconnect" href="https://fonts.googleapis.com">    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>    <style>        * { margin: 0; padding: 0; box-sizing: border-box; }        :root { --primary-orange: #FF8C42; --light-orange: #FFB88C; --dark-orange: #E67E22; --grey-dark: #1F2937; --grey-medium: #4B5563; --grey-light: #F3F4F6; --grey-lighter: #FAFBFC; --white: #FFFFFF; --dark-bg: #0F1419; }        html { scroll-behavior: smooth; }        body { font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', sans-serif; line-height: 1.6; color: var(--grey-dark); background: var(--white); overflow-x: hidden; }        nav { position: fixed; top: 0; width: 100%; background: rgba(255, 255, 255, 0.95); backdrop-filter: blur(20px); padding: 1rem 3rem; display: flex; justify-content: space-between; align-items: center; z-index: 1000; box-shadow: 0 2px 8px rgba(0, 0, 0, 0.04); border-bottom: 1px solid rgba(0, 0, 0, 0.05); }        nav .logo { font-size: 1.5rem; font-weight: 700; background: linear-gradient(135deg, var(--primary-orange), var(--dark-orange)); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text; text-decoration: none; text-transform: uppercase; font-style: italic; }        nav ul { list-style: none; display: flex; gap: 2.5rem; }        nav a { color: var(--grey-dark); text-decoration: none; font-weight: 500; font-size: 0.95rem; transition: all 0.3s ease; position: relative; }        nav a::after { content: ''; position: absolute; bottom: -8px; left: 0; width: 0; height: 2px; background: linear-gradient(90deg, var(--primary-orange), var(--dark-orange)); transition: width 0.4s cubic-bezier(0.34, 1.56, 0.64, 1); }        nav a:hover::after { width: 100%; }        nav a:hover { color: var(--primary-orange); }        .hero { margin-top: 60px; min-height: 100vh; background: linear-gradient(135deg, var(--grey-lighter) 0%, var(--white) 50%, rgba(255, 140, 66, 0.03) 100%); display: flex; align-items: center; justify-content: center; padding: 4rem 2rem; position: relative; overflow: hidden; }        .hero::before { content: ''; position: absolute; top: -200px; right: -100px; width: 600px; height: 600px; background: radial-gradient(circle, rgba(255, 140, 66, 0.12), transparent 70%); border-radius: 50%; animation: float 8s ease-in-out infinite; }        .hero::after { content: ''; position: absolute; bottom: -150px; left: -50px; width: 500px; height: 500px; background: radial-gradient(circle, rgba(255, 140, 66, 0.08), transparent 70%); border-radius: 50%; animation: float 10s ease-in-out infinite reverse; }        .hero-content { max-width: 1100px; position: relative; z-index: 1; text-align: center; animation: fadeInScale 1.2s ease; }        .hero h1 { font-size: 4.5rem; color: var(--grey-dark); margin-bottom: 1.5rem; font-weight: 800; line-height: 1.1; letter-spacing: -1px; animation: slideInDown 1s ease; }        .hero h1 .gradient-text { background: linear-gradient(135deg, var(--primary-orange), var(--dark-orange)); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text; }        .hero p { font-size: 1.4rem; color: var(--grey-medium); margin-bottom: 2.5rem; line-height: 1.6; max-width: 700px; margin-left: auto; margin-right: auto; animation: slideInUp 1s ease 0.2s both; }        .hero-actions { display: flex; gap: 1.5rem; justify-content: center; margin-bottom: 4rem; animation: slideInUp 1s ease 0.4s both; }        .cta-button { display: inline-block; padding: 1.2rem 3rem; background: linear-gradient(135deg, var(--primary-orange), var(--dark-orange)); color: white; text-decoration: none; border-radius: 12px; font-weight: 600; font-size: 1rem; transition: all 0.3s cubic-bezier(0.34, 1.56, 0.64, 1); box-shadow: 0 8px 24px rgba(255, 140, 66, 0.3); border: 2px solid transparent; }        .cta-button:hover { transform: translateY(-4px); box-shadow: 0 12px 32px rgba(255, 140, 66, 0.4); }        .cta-button.secondary { background: transparent; color: var(--primary-orange); border: 2px solid var(--primary-orange); box-shadow: none; }        .cta-button.secondary:hover { background: var(--primary-orange); color: white; box-shadow: 0 8px 24px rgba(255, 140, 66, 0.3); }        .demo-container { margin-top: 4rem; background: white; border-radius: 20px; padding: 3rem 2rem; box-shadow: 0 20px 60px rgba(0, 0, 0, 0.08); animation: slideInUp 1s ease 0.6s both; border: 1px solid rgba(0, 0, 0, 0.05); }        .demo-container h3 { text-align: center; color: var(--primary-orange); margin-bottom: 2rem; font-size: 1.5rem; font-weight: 600; }        section { padding: 140px 2rem; position: relative; overflow: hidden; }        .section-wrapper { max-width: 1300px; margin: 0 auto; }        .section-title { font-size: 3.2rem; color: var(--grey-dark); margin-bottom: 1.5rem; text-align: center; font-weight: 800; line-height: 1.2; opacity: 0; transform: translateY(40px); }        .section-title.revealed { animation: revealDown 1s cubic-bezier(0.34, 1.56, 0.64, 1) forwards; }        .section-title .gradient-text { background: linear-gradient(135deg, var(--primary-orange), var(--dark-orange)); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text; }        .section-subtitle { font-size: 1.2rem; color: var(--grey-medium); text-align: center; margin-bottom: 5rem; max-width: 700px; margin-left: auto; margin-right: auto; line-height: 1.7; opacity: 0; transform: translateY(40px); }        .section-subtitle.revealed { animation: revealDown 1s cubic-bezier(0.34, 1.56, 0.64, 1) 0.2s forwards; }        .services { background: linear-gradient(to bottom, var(--white) 0%, var(--grey-lighter) 100%); }        .services-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(320px, 1fr)); gap: 2.5rem; margin-top: 4rem; }        .service-card { background: white; padding: 3rem; border-radius: 20px; box-shadow: 0 4px 16px rgba(0, 0, 0, 0.05); transition: all 0.4s cubic-bezier(0.34, 1.56, 0.64, 1); position: relative; overflow: hidden; border: 1px solid rgba(0, 0, 0, 0.03); opacity: 0; transform: translateY(40px); }        .service-card.revealed { animation: revealUp 0.8s cubic-bezier(0.34, 1.56, 0.64, 1) forwards; }        .service-card:nth-child(n) { animation-delay: calc(n * 0.1s); }        .service-card::before { content: ''; position: absolute; top: 0; left: 0; width: 100%; height: 4px; background: linear-gradient(90deg, var(--primary-orange), var(--dark-orange)); transform: scaleX(0); transform-origin: left; transition: transform 0.4s cubic-bezier(0.34, 1.56, 0.64, 1); }        .service-card:hover::before { transform: scaleX(1); }        .service-card:hover { transform: translateY(-16px); box-shadow: 0 24px 48px rgba(255, 140, 66, 0.15); border-color: rgba(255, 140, 66, 0.1); }        .service-icon { font-size: 3.5rem; background: linear-gradient(135deg, var(--primary-orange), var(--dark-orange)); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text; margin-bottom: 1.5rem; transition: all 0.3s ease; }        .service-card:hover .service-icon { transform: scale(1.1); }        .service-card h3 { font-size: 1.4rem; color: var(--grey-dark); margin-bottom: 1rem; font-weight: 700; }        .service-card p { color: var(--grey-medium); line-height: 1.8; font-size: 0.95rem; }        .products { background: var(--white); }        .products-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(280px, 1fr)); gap: 3rem; margin-top: 4rem; }        .product-card { background: white; border-radius: 20px; overflow: hidden; box-shadow: 0 4px 16px rgba(0, 0, 0, 0.05); transition: all 0.4s cubic-bezier(0.34, 1.56, 0.64, 1); opacity: 0; transform: scale(0.9); border: 1px solid rgba(0, 0, 0, 0.03); }        .product-card.revealed { animation: revealScale 0.8s cubic-bezier(0.34, 1.56, 0.64, 1) forwards; }        .product-card:hover { transform: translateY(-20px); box-shadow: 0 32px 64px rgba(255, 140, 66, 0.2); }        .product-image { width: 100%; height: 240px; background: linear-gradient(135deg, var(--light-orange), var(--primary-orange)); display: flex; align-items: center; justify-content: center; font-size: 4.5rem; transition: all 0.4s ease; }        .product-card:hover .product-image { transform: scale(1.05); }        .product-info { padding: 2.5rem; }        .product-info h3 { font-size: 1.3rem; color: var(--grey-dark); margin-bottom: 0.8rem; font-weight: 700; }        .product-info p { color: var(--grey-medium); font-size: 0.9rem; line-height: 1.7; }        .about { background: linear-gradient(to bottom, var(--grey-lighter) 0%, var(--white) 100%); }        .about-content { display: grid; grid-template-columns: 1fr 1fr; gap: 5rem; align-items: center; margin-top: 4rem; }        .about-text { opacity: 0; transform: translateX(-60px); }        .about-text.revealed { animation: revealLeft 1s cubic-bezier(0.34, 1.56, 0.64, 1) forwards; }        .about-text h3 { font-size: 1.6rem; color: var(--grey-dark); margin-bottom: 1.5rem; font-weight: 700; margin-top: 1.5rem; }        .about-text h3:first-child { margin-top: 0; }        .about-image { background: linear-gradient(135deg, var(--light-orange), var(--primary-orange)); border-radius: 20px; width: 100%; height: 500px; display: flex; align-items: center; justify-content: center; font-size: 7rem; opacity: 0; transform: translateX(60px); box-shadow: 0 20px 60px rgba(255, 140, 66, 0.2); }        .about-image.revealed { animation: revealRight 1s cubic-bezier(0.34, 1.56, 0.64, 1) forwards; }        .clients { background: var(--white); }        .clients-grid { display: grid; grid-template-columns: repeat(auto-fit, minmax(240px, 1fr)); gap: 2.5rem; margin-top: 4rem; }        .client-card { background: var(--grey-lighter); padding: 2.5rem; border-radius: 20px; text-align: center; box-shadow: 0 4px 16px rgba(0, 0, 0, 0.04); transition: all 0.4s cubic-bezier(0.34, 1.56, 0.64, 1); border: 2px solid transparent; opacity: 0; transform: translateY(40px); }        .client-card.revealed { animation: revealUp 0.8s cubic-bezier(0.34, 1.56, 0.64, 1) forwards; }        .client-card:hover { border-color: var(--primary-orange); box-shadow: 0 16px 40px rgba(255, 140, 66, 0.15); transform: translateY(-8px); background: white; }        .client-logo { font-size: 3.5rem; background: linear-gradient(135deg, var(--primary-orange), var(--dark-orange)); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text; margin-bottom: 1.5rem; transition: transform 0.3s ease; }        .client-card:hover .client-logo { transform: scale(1.1); }        .client-card h3 { font-size: 1.2rem; color: var(--grey-dark); margin-bottom: 0.8rem; font-weight: 700; }        .client-card p { color: var(--grey-medium); font-size: 0.85rem; line-height: 1.7; }        .contact { background: linear-gradient(135deg, var(--grey-dark) 0%, var(--grey-medium) 100%); color: white; position: relative; }        .contact::before { content: ''; position: absolute; top: 0; right: 0; width: 400px; height: 400px; background: radial-gradient(circle, rgba(255, 140, 66, 0.1), transparent 70%); border-radius: 50%; animation: float 8s ease-in-out infinite; }        .contact .section-title { color: white; }        .contact .section-subtitle { color: rgba(255, 255, 255, 0.85); }        .contact-content { display: grid; grid-template-columns: repeat(auto-fit, minmax(300px, 1fr)); gap: 2.5rem; margin-top: 4rem; position: relative; z-index: 1; }        .contact-item { background: rgba(255, 255, 255, 0.08); padding: 3rem; border-radius: 20px; border: 1px solid rgba(255, 255, 255, 0.15); backdrop-filter: blur(20px); transition: all 0.4s cubic-bezier(0.34, 1.56, 0.64, 1); text-align: center; opacity: 0; transform: translateY(40px); }        .contact-item.revealed { animation: revealUp 0.8s cubic-bezier(0.34, 1.56, 0.64, 1) forwards; }        .contact-item:hover { background: rgba(255, 140, 66, 0.15); border-color: var(--primary-orange); transform: translateY(-12px); box-shadow: 0 20px 50px rgba(255, 140, 66, 0.2); }        .contact-icon { font-size: 2.8rem; color: var(--light-orange); margin-bottom: 1.5rem; transition: all 0.3s ease; }        .contact-item:hover .contact-icon { transform: scale(1.15) rotate(5deg); }        .contact-item h3 { font-size: 1.3rem; margin-bottom: 1rem; font-weight: 700; }        .contact-item p { font-size: 0.95rem; opacity: 0.95; line-height: 1.7; }        .contact-item a { color: var(--light-orange); text-decoration: none; transition: all 0.3s ease; font-weight: 500; }        .contact-item a:hover { opacity: 0.8; text-decoration: underline; }        footer { background: var(--dark-bg); color: white; padding: 4rem 2rem 2rem; text-align: center; border-top: 1px solid rgba(255, 140, 66, 0.2); }        .footer-brand { font-size: 1.3rem; font-weight: 700; background: linear-gradient(135deg, var(--primary-orange), var(--dark-orange)); -webkit-background-clip: text; -webkit-text-fill-color: transparent; background-clip: text; margin-bottom: 1rem; text-transform: uppercase; font-style: italic; }        footer p { opacity: 0.8; margin-bottom: 1rem; font-size: 0.9rem; }        @@keyframes float { 0%, 100% { transform: translateY(0); } 50% { transform: translateY(40px); } }        @@keyframes slideInDown { from { opacity: 0; transform: translateY(-40px); } to { opacity: 1; transform: translateY(0); } }        @@keyframes slideInUp { from { opacity: 0; transform: translateY(40px); } to { opacity: 1; transform: translateY(0); } }        @@keyframes fadeInScale { from { opacity: 0; transform: scale(0.95); } to { opacity: 1; transform: scale(1); } }        @@keyframes revealDown { from { opacity: 0; transform: translateY(40px); } to { opacity: 1; transform: translateY(0); } }        @@keyframes revealUp { from { opacity: 0; transform: translateY(40px); } to { opacity: 1; transform: translateY(0); } }        @@keyframes revealLeft { from { opacity: 0; transform: translateX(-60px); } to { opacity: 1; transform: translateX(0); } }        @@keyframes revealRight { from { opacity: 0; transform: translateX(60px); } to { opacity: 1; transform: translateX(0); } }        @@keyframes revealScale { from { opacity: 0; transform: scale(0.9); } to { opacity: 1; transform: scale(1); } }        @@media (max-width: 768px) { nav { padding: 1rem 1.5rem; } nav ul { gap: 1rem; } .hero h1 { font-size: 2.5rem; } .hero p { font-size: 1rem; } .hero-actions { flex-direction: column; } .cta-button { min-width: 250px; } .section-title { font-size: 2rem; } .services-grid, .products-grid, .clients-grid { grid-template-columns: 1fr; } .about-content { grid-template-columns: 1fr; gap: 2rem; } .about-image { height: 250px; } section { padding: 80px 1rem; } }    </style></head><body>    <nav>        <a href="#" class="logo">Greybeard</a>        <ul>            <li><a href="#services">Services</a></li>            <li><a href="#products">Products</a></li>            <li><a href="#about">About</a></li>            <li><a href="#clients">Clients</a></li>            <li><a href="#contact">Contact</a></li>        </ul>    </nav>    <section id="home" class="hero">        <div class="hero-content">            <h1>@Model.Value("heroTitle")</h1>            <p>@Model.Value("heroSubtitle")</p>            <div class="hero-actions">                <a href="#contact" class="cta-button">Get Started</a>                <a href="#services" class="cta-button secondary">Explore Services</a>            </div>            <div class="demo-container">                <h3>Try Our Interactive Demo</h3>                @Html.Raw(Model.Value("demoEmbedCode"))            </div>        </div>    </section>    <section id="services" class="services">        <div class="section-wrapper">            <h2 class="section-title">@Model.Value("servicesTitle")</h2>            <p class="section-subtitle">Comprehensive solutions tailored to your needs</p>            <div class="services-grid">                <div class="service-card">                    <div class="service-icon"><i class="fas fa-pencil-ruler"></i></div>                    <h3>Web Design</h3>                    <p>Beautiful, responsive websites that engage your audience and drive conversions.</p>                </div>                <div class="service-card">                    <div class="service-icon"><i class="fas fa-code"></i></div>                    <h3>Development</h3>                    <p>Clean, scalable code built with the latest technologies and best practices.</p>                </div>                <div class="service-card">                    <div class="service-icon"><i class="fas fa-paint-brush"></i></div>                    <h3>Branding</h3>                    <p>Strategic brand identity design that sets you apart from the competition.</p>                </div>                <div class="service-card">                    <div class="service-icon"><i class="fas fa-mobile-alt"></i></div>                    <h3>Mobile Apps</h3>                    <p>Native and cross-platform apps that deliver exceptional user experiences.</p>                </div>                <div class="service-card">                    <div class="service-icon"><i class="fas fa-chart-line"></i></div>                    <h3>Digital Strategy</h3>                    <p>Data-driven strategies to grow your online presence and achieve goals.</p>                </div>                <div class="service-card">                    <div class="service-icon"><i class="fas fa-headset"></i></div>                    <h3>Support</h3>                    <p>Dedicated support to ensure your projects run smoothly at all times.</p>                </div>            </div>            <div style="margin-top: 4rem;">                @Html.Raw(Model.Value("servicesContent"))            </div>        </div>    </section>    <section id="products" class="products">        <div class="section-wrapper">            <h2 class="section-title">@Model.Value("productsTitle")</h2>            <p class="section-subtitle">Innovative solutions crafted with precision</p>            <div class="products-grid">                <div class="product-card">                    <div class="product-image">🎨</div>                    <div class="product-info">                        <h3>Design System</h3>                        <p>Complete design system for consistent branding across all platforms.</p>                    </div>                </div>                <div class="product-card">                    <div class="product-image">⚙️</div>                    <div class="product-info">                        <h3>Dev Tools</h3>                        <p>Powerful development tools to accelerate your workflow.</p>                    </div>                </div>                <div class="product-card">                    <div class="product-image">📱</div>                    <div class="product-info">                        <h3>Mobile SDK</h3>                        <p>Comprehensive SDK for seamless mobile app development.</p>                    </div>                </div>                <div class="product-card">                    <div class="product-image">📊</div>                    <div class="product-info">                        <h3>Analytics Suite</h3>                        <p>Advanced analytics to understand your users better.</p>                    </div>                </div>            </div>            <div style="margin-top: 4rem;">                @Html.Raw(Model.Value("productsContent"))            </div>        </div>    </section>    <section id="about" class="about">        <div class="section-wrapper">            <h2 class="section-title">@Model.Value("aboutTitle")</h2>            <div class="about-content">                <div class="about-text">                    @Html.Raw(Model.Value("aboutContent"))                </div>                <div class="about-image">🚀</div>            </div>        </div>    </section>    <section id="clients" class="clients">        <div class="section-wrapper">            <h2 class="section-title">@Model.Value("clientsTitle")</h2>            <p class="section-subtitle">Trusted by leading companies worldwide</p>            <div class="clients-grid">                <div class="client-card">                    <div class="client-logo">🏢</div>                    <h3>Tech Startup</h3>                    <p>Revolutionizing the industry with innovative solutions</p>                </div>                <div class="client-card">                    <div class="client-logo">🏦</div>                    <h3>Financial Corp</h3>                    <p>Delivering excellence in financial services</p>                </div>                <div class="client-card">                    <div class="client-logo">🛍️</div>                    <h3>E-commerce Plus</h3>                    <p>Leading online retail transformation</p>                </div>                <div class="client-card">                    <div class="client-logo">🏥</div>                    <h3>Health Systems</h3>                    <p>Improving healthcare delivery globally</p>                </div>            </div>            <div style="margin-top: 4rem;">                @Html.Raw(Model.Value("clientsContent"))            </div>        </div>    </section>    <section id="contact" class="contact">        <div class="section-wrapper">            <h2 class="section-title">Get In Touch</h2>            <p class="section-subtitle">Ready to start your next project? Contact us today</p>            <div class="contact-content">                <div class="contact-item">                    <div class="contact-icon"><i class="fas fa-envelope"></i></div>                    <h3>Email</h3>                    <p><a href="mailto:@Model.Value("contactEmail")">@Model.Value("contactEmail")</a></p>                </div>                <div class="contact-item">                    <div class="contact-icon"><i class="fas fa-phone"></i></div>                    <h3>Phone</h3>                    <p><a href="tel:@Model.Value("contactPhone")">@Model.Value("contactPhone")</a></p>                </div>                <div class="contact-item">                    <div class="contact-icon"><i class="fas fa-map-marker-alt"></i></div>                    <h3>Location</h3>                    <p>@Model.Value("contactAddress")</p>                </div>            </div>        </div>    </section>    <footer>        <div class="footer-content">            <div class="footer-brand">Greybeard</div>            <p>&copy; 2025 Greybeard. All rights reserved.</p>            <p>Creating beautiful, functional digital experiences</p>        </div>    </footer>    <script>        const observerOptions = { threshold: 0.1, rootMargin: '0px 0px -100px 0px' };        const observer = new IntersectionObserver(function(entries) {            entries.forEach(entry => {                if (entry.isIntersecting) {                    entry.target.classList.add('revealed');                    observer.unobserve(entry.target);                }            });        }, observerOptions);        document.querySelectorAll('.section-title, .section-subtitle, .service-card, .product-card, .client-card, .contact-item, .about-text, .about-image').forEach(el => {            observer.observe(el);        });    </script></body></html>